<div class="container">
  <div class="row">
    <h3>Images</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @images.each do |image| %>
            <tr>
              <%= render image %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
